package com.rapidraiderscores.rapidraiderscores.entities;

public class OtpEntity {
	
	

	private int otp;
	private UserRegister uregister;
	
	
	
	public OtpEntity(int otp, UserRegister uregister) {
		super();
		this.otp = otp;
		this.uregister = uregister;
	}


	public int getOtp() {
		return otp;
	}


	public void setOtp(int otp) {
		this.otp = otp;
	}


	public UserRegister getUregister() {
		return uregister;
	}


	public void setUregister(UserRegister uregister) {
		this.uregister = uregister;
	}


	public OtpEntity() {
		super();
		// TODO Auto-generated constructor stub
	}


	@Override
	public String toString() {
		return "OtpEntity [otp=" + otp + ", uregister=" + uregister + "]";
	}

	
	

}
